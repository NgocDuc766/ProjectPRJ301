<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>controller.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateProfileStudent</servlet-name>
        <servlet-class>controller.UpdateProfileStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleServlet</servlet-name>
        <servlet-class>controller.ScheduleStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailActivityServlet</servlet-name>
        <servlet-class>controller.DetailActivityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewClassStudent</servlet-name>
        <servlet-class>controller.ViewClassStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewClassHome</servlet-name>
        <servlet-class>controller.ViewClassHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleTeacher</servlet-name>
        <servlet-class>controller.ScheduleTeacherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailAttendanceServlet</servlet-name>
        <servlet-class>controller.DetailAttendanceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveAttendanceServlet</servlet-name>
        <servlet-class>controller.SaveAttendanceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminAccount</servlet-name>
        <servlet-class>controller.AdminAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewScheduleServlet</servlet-name>
        <servlet-class>controller.AddNewScheduleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadClassServlet</servlet-name>
        <servlet-class>controller.UploadClassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteScheduleServlet</servlet-name>
        <servlet-class>controller.DeleteScheduleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateProfileStudent</servlet-name>
        <url-pattern>/updatestu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleServlet</servlet-name>
        <url-pattern>/schedulestudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailActivityServlet</servlet-name>
        <url-pattern>/detailActivity</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewClassStudent</servlet-name>
        <url-pattern>/viewclass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewClassHome</servlet-name>
        <url-pattern>/classhome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleTeacher</servlet-name>
        <url-pattern>/scheduleteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailAttendanceServlet</servlet-name>
        <url-pattern>/attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveAttendanceServlet</servlet-name>
        <url-pattern>/confirmTeacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminAccount</servlet-name>
        <url-pattern>/manageaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewScheduleServlet</servlet-name>
        <url-pattern>/addschedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadClassServlet</servlet-name>
        <url-pattern>/uploadclass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteScheduleServlet</servlet-name>
        <url-pattern>/deleteschedule</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
